{% comment %}
  DO NOT MODIFY THIS FILE DIRECTLY. THIS FILE WILL BE OVERWRITTEN.
  YOU CAN MODIFY THE LIQUID CODE FOR THIS CUSTOM FIELD HERE:
  https://customfields.bonify.io/dashboard/gittiberlin.myshopify.com/products/configure/130839/edit
{% endcomment %}
{% if product.metafields.custom_fields["preorder"] != blank %}
  <div class="custom-field custom-field__preorder custom-field__type--checkbox">
    <h3 class="custom-field--title">preorder</h3>
    <div class="custom-field--value">
      {% if product.metafields.custom_fields["preorder"] == 1 %}
        TRUE
      {% else %}
        FALSE
      {% endif %}
    </div>
  </div>
{% endif %}