{% assign id = section.id %}

<style>
     #shopify-section-{{id}} .bc-object-heading {
    padding-top: {{section.settings.padding-top-heading}}px;
    padding-bottom: {{section.settings.padding-bottom-heading}}px;
    color: {{section.settings.text-color-heading}};
}
#shopify-section-{{id}} .bc-object-heading p{
    color: {{section.settings.text-color-heading}};
}
#shopify-section-{{id}} .bc-object-richtext {
    padding-top: {{section.settings.padding-top-text}}px;
    padding-bottom: {{section.settings.padding-bottom-text}}px;
    color: {{section.settings.text-color-text}};
}
#shopify-section-{{id}} .bc-object-richtext p{
    color: {{section.settings.text-color-text}};
}
</style>

{% for block in section.blocks %}

    <div id="shopify-section-{{block.id}}" {{block.shopify_attributes}}>
        <div class="page-width {% if block.settings.is_hero  %}mobile_noborder{% endif %}">
            {% case block.type %}
                {% when 'image_with_text' %}
                    {% render 'object_image_with_text', object: block %}
                {% when 'image_hero_page' %}
                    {% render 'object_image_hero', object: block %}
                {% when 'heading' %}
                    {% render 'object_heading', object: block %}
                {% when 'richtext' %}
                    {% render 'object_richtext', object: block %}
                {% when 'double_content' %}
                    {% render 'object_double_content', object: block %}
                {% when 'image_text_alt' %}
                    {% render 'object_image_with_text_altern', object: block %}
                    {% comment %}more fuctionality for gitti design{% endcomment %}
                    <style>
                        #shopify-section-{{block.id}} + .page-width{
                            margin: 0px;
                            padding: 0px;
                            max-width: initial;
                        }
                    </style>
                {% when 'd_content_alt' %}
                    {% render 'object_double_content_altern', object: block %}
                    {% comment %}more fuctionality for gitti design{% endcomment %}
                {% when 'gallery' %}
                    {% render 'object_gallery', object: block %}
                    {% comment %}more fuctionality for gitti design{% endcomment %}
                {% when 'info_slider' %}
                    {% render 'object_page_info_slider', object: block %}
                    {% comment %}more fuctionality for gitti design{% endcomment %}
                {% when 'tabs' %}
                    {% render 'object_tabs', object: block %}
                    {% comment %}more fuctionality for gitti design{% endcomment %}
                {% when 'info_icons' %}
                    {% render 'object_page_info_icons', object: block %}
                    {% comment %}more fuctionality for gitti design{% endcomment %}
                {% when 'single_tab' %}
                    {% render 'object_single_tab', object: block %}
                {% when 'video_text' %}
                    {% render 'object_video_with_text', object: block %}
                {% when 'html_content' %}
                    {% render 'object_custom_html', object: block %}
                {% when 'collections' %}
                    {% render 'object_page_collections', object: block %}
                {% when 'testimonials' %}
                    {% render 'object_page_testimonials', object: block %}
                {% when 'products' %}
                    {% render 'object_page_collection_products', object: block %}
                    <style>
                        #tns2-mw{
                            overflow: inherit;
                            clip-path: inset( -100vw -100vw -100vw 0 );
                        }

                        #shopify-section-{{block.id}} > .page-width{
                            margin: 0px;
                            padding: 0px;
                            max-width: initial;
                        }

                    </style>

               {% when 'triple_usp' %}
                   {% render 'object_triple_usp_lip_sync', object: block %}
               {% when 'Why-gitti' %}
                   {% render 'object_why_gitti_lip_sync', object: block %}
                {% when 'featured_promotion' %}
                   {% render 'object_featured_promotions_lip_sync', object: block %}
                {% when 'press_download' %}
                   {% render 'object_press_download_lip_sync', object: block %}
                 {% when 'shop_the_look' %}
                   {% render 'object_shop_the_look_lip_sync', object: block %}

                   {% when 'video_page' %}
                   {% render 'object_video_lip_sync', object: block %}
                   {% if block.settings.ignore_page_width %}
                   <style>
                       #shopify-section-{{block.id}} .page-width{
                           margin: 0px;
                           padding: 0px;
                           max-width: initial;
                       }
                   </style>
               {% endif %}

               {% when 'hero_slideshow' %}
               {% render 'object_hero_slideshow_lip_sync', object: block %}

               {% when 'feature_product' %}
               {% render 'object_feature_product_lip_sync', object: block %}
            {% endcase %}
            </div>
    </div>

{% endfor %}


{% if request.design_mode %}
    <script>
        window.addEventListener('shopify:block:select', function(event){
            var ele = document.querySelector('#shopify-section-'+event.detail.blockId);
            window.scrollTo(ele.offsetLeft,ele.offsetTop);
        });
    </script>
{% endif %}

